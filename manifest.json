{
  "name": "QuickTip",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "120",
  "description": "Adds a formatted tooltip to selected values",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["./bundle.js"],
    "css": ["./style.css"],
    "type": "module"
  }],
  "host_permissions": ["<all_urls>"],
  "action": {}
}
